@{
	var taskId = ViewData["taskId"];
	var taskJson = ViewData["TaskJson"] as string;
	ViewData["Title"] = "Szczegóły zgłoszenia " + taskId;
	var userJson = Context.Session.GetString("User");
}
@model UserModel

<div class="tasksData" data-task="@taskJson" id="taskDetails"></div>
<div class="userData" data-user="@userJson"></div>
<div class="upperpanel">
	<div class="sitename">
		<h2>Szczegóły zgłoszenia #@taskId</h2>
	</div>
	<a href="@Url.Action("Logout","User")">
		<div class="function">
			Wyloguj
		</div>
	</a>
</div>
<div class="text-center" id="taskDetailsDiv">
</div>
<div class="returnButton">
	<a asp-area="" asp-controller="User" asp-action="UserMenu">
		<div class="returnMenu">
			<h4>Powrót do menu</h4>
		</div>
	</a>
</div>